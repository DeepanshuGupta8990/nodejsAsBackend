<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
      input{
        display: block;
        margin: 10px;
      }
      #btn1{
        margin: 10px;
      }
    </style>
</head>
<body>
    <h1>Hello world!</h1>
    <input type="text" name="name1" id="name1">
    <input type="number" name="age1" id="age1">
    <input type="text" name="name2" id="name2">
    <input type="number" name="name3" id="age2">
    <button id="btn1">Send</button>

</body>
<script>

  let btn1 = document.getElementById("btn1");

  btn1.addEventListener('click',()=>{
    let name1 = document.getElementById('name1').value
  let age1 = document.getElementById('age1').value
  let name2 = document.getElementById('name2').value
  let age2 = document.getElementById('age2').value
    console.log(name1)
    fetch(`http://localhost:4500/users/all?name=${name1}&age=${age1}`, {
  method: 'POST',
  headers: {
    'Content-Type': 'application/json'
  },
  body: JSON.stringify({ name: `${name2}`, age: age2 }),
}) .then(response => response.json())
  .then(data=> console.log(data))
  .catch(error => console.error(error));
  })


</script>
</html>